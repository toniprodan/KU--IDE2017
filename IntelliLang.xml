<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Apple JS" injector-id="xml">
      <display-name>head</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("head")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="CSS" injector-id="xml">
      <display-name>svg/@id</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("id")).withParent(xmlTag().withLocalName(string().equalTo("svg")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>